<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>JavaDeathValley</display-name>

  <servlet>
    <servlet-name>ServletTest</servlet-name>
    <servlet-class>ServletTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletTest</servlet-name>
    <url-pattern>/ServletTest</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>AllUsers</servlet-name>
    <servlet-class>servlets.AllUsers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AllUsers</servlet-name>
    <url-pattern>/AllUsers</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>UserById</servlet-name>
    <servlet-class>servlets.UserById</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserById</servlet-name>
    <url-pattern>/UserById</url-pattern>
  </servlet-mapping>






</web-app>
